setup.removeAllLinks = function removeLinks(links) {
    links.each(function() {
        /* Remove data attributes and thus handler for keyup and remove sups */
        $(this).find('sup').remove();
        $(this).removeAttr("data-link-number");
    });
};
setup.removeLinks = function removeLinks(links) {
    links.each(function() {
        /* Remove sups */
        $(this).find('sup').remove();
    });
};
setup.updateLinks = function updateLinks() {
    /* Manual overwrite when a passage should not have numbered links */
    let allLinks = $(".passage a");
    if (!settings.NumberedLinks) {
        allLinks.each(function() {
            /* Remove data attributes and thus handler for keyup and remove sups */
            $(this).find('sup').remove();
        });
    }
    if (!tags().includes("DisableKeyLinks")) {
        /* Find all links inside the passage class */
        for (let step = 0; step < allLinks.length; step++) {
            if (!$(allLinks[step]).parents().hasClass("macro-type") || $(allLinks[step]).parents().hasClass("macro-type-done")) {
                let linkText = allLinks.eq(step).text();
                if (step === 10) {
                    /* When step is ten, we need manual 0 */
                    allLinks.eq(10).html(linkText + "<sup>[0]</sup>");
                    allLinks.eq(10).attr("data-link-number", "0");
                } else {
                    /* Add the sup element to any link found with the data attribute */
                    if (settings.NumberedLinks) {
                        allLinks.eq(step).html(linkText + "<sup>["+(step + 1)+"]</sup>");
                    }
                    allLinks.eq(step).attr("data-link-number", step);
                }
            }            
        }
    } 
};
$(document).on(':typingcomplete', function (ev) {
	setup.removeAllLinks($(".passage a"))
    if (!tags().includes("DisableKeyLinks")) {
        let allLinks = $(".passage a");
        /* Find all links inside the passage class */
        for (let step = 0; step < allLinks.length; step++) {
            let linkText = allLinks.eq(step).text();
            if (step === 10) {
                /* When step is ten, we need manual 0 */
                allLinks.eq(10).html(linkText + "<sup>[0]</sup>");
                allLinks.eq(10).attr("data-link-number", "0");
            } else {
                /* Add the sup element to any link found with the data attribute */
                if (settings.NumberedLinks) {
                    allLinks.eq(step).html(linkText + "<sup>["+(step + 1)+"]</sup>");
                }
                allLinks.eq(step).attr("data-link-number", step);
            }           
        }
    } 
});
$(document).on(':passageend', function (ev) {
    /* Call function every ending of DOM creation, so no weirdness can happen */
	setup.updateLinks();
    const config = {
        subtree: true, childList: true, characterData: true
    };
    const targetNode = document.getElementById("passages");
      
    function observerCallback(mutationList) {
        mutationList.forEach(mutation => {
        if (mutation.type === "childList") {
            if ($(mutation.target).is("a")) {
            // link has had children. ignore
            } else {
                setup.removeAllLinks($(".passage a"))
                setup.updateLinks();
            }
        }
        });
    }
    
    const MO = new MutationObserver(observerCallback);
    MO.observe(targetNode, config);

    if($('.macro-cycle').length){
        $('.macro-cycle').on("click", function() {
            setup.removeAllLinks($(".passage a"));
            setup.updateLinks()
        });
    }
});
$(document).on("keyup", function (e) {
    /* We need a preventDefault() to prevent ghost clicking */
    e.preventDefault();
    /* Keys mapped to index */
    let keyMapNormal = [49, 50, 51, 52,  53, 54, 55, 56, 57, 48];
