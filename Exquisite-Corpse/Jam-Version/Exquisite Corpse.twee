:: StoryStylesheet [stylesheet]
@import url('https://fonts.googleapis.com/css2?family=Georgia&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@300&display=swap');

:root {
  /* (default) light mode */
  --bg: #ebe3d9;
  --text: #000;
  --link: #1a1a1a;
  --accent: #f0e6e6;
  --accent2: #d5d5d5;
  /* dark mode */
  --dm-bg: #000;
  --dm-text: #fff;
  --dm-link: #fff;
  --dm-accent: #1a1a1a;
  --dm-accent2: #111111;
}
  
/* body */
body {
  background-color: var(--bg);
  color: var(--text);
    font-family: 'Georgia', serif;
  font-size: 20px;
}
.dm body {
  background-color: var(--dm-bg);
  color: var(--dm-text);
}
.sanserif body {
  font-family: 'Montserrat', sans-serif;
}

/* passages */
.passage {
  overflow-y: scroll; /* allows the player to scroll within the passage, rather than the overall page */
  border: 1px solid var(--accent);
  border-radius: 5px;
  padding: 1em;
  height: auto;
  width: auto;
  background-color: var(--accent2);
  /* hides the scrollbars in various browsers - will still scroll. to show scrollbars, remove these two lines. */
  scrollbar-width: none;
  -ms-overflow-style: none;
}
.dm .passage {
  background-color: var(--dm-accent2);
  border: 1px solid var(--dm-accent);
}

/* links */
a {
  color: var(--link);
  font-weight: bold;
}
a:hover {
  font-weight: bold;
  text-decoration: underline;
  color: var(--link);
}
.dm a {
  color: var(--dm-link);
}
.dm a:hover {
  color: var(--dm-link);
}

/* creates a div that contains the links as shown in the passages - if you don't use a div container for your links, feel free to delete this css up to the ui bar styling and make sure to adjust the hover colours of the link css */
.choice {
  border: 1px solid var(--accent);
  border-radius: 5px;
  background-color: transparent;
  color: black;
  padding: 1em;
  margin-top: .1em;
}
.choice:before {
  content: "";
  padding: 1em;
}
.choice:hover {
  background-color: var(--accent);
  transition: .5s linear;
}
.dm .choice {
  border: 1px solid var(--dm-accent);
}
.dm .choice:hover {
  background-color: var(--dm-accent);
}

/* ui bar */
#story {
	margin-left: 20em;
	transition: margin-left 200ms ease-in;
}
#ui-bar.stowed ~ #story {
	margin-left: 4.5em;
}
@media screen and (max-width: 1136px) {
	#story {
		margin-left: 19em;
	}
	#ui-bar.stowed ~ #story {
		margin-left: 3.5em;
	}
}

@media screen and (max-width: 768px) {
	#story {
		margin-left: 3.5em;
	}
}


#ui-bar {
  background-color: var(--accent);
  color: var(--text);
  background-image: url('https://images.unsplash.com/photo-1462642109801-4ac2971a3a51?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=966&q=80');
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.dm #ui-bar {
  background-color: var(--dm-accent);
  color: var(--dm-text);
  background-image: url('https://images.unsplash.com/photo-1587135991058-8816b028691f?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=634&q=80');
}

}
#ui-bar.stowed {
	left: -15.5em;
}
#ui-bar-tray {
	position: absolute;
	top: 0.2em;
	left: 0;
	right: 0;
}
#ui-bar-body {
	height: 90%; /* fallback for browsers without support for calc() */
	height: calc(100% - 2.5em);
	margin: 2.5em 0;
	padding: 0 1.5em;
}
#ui-bar.stowed #ui-bar-history,
#ui-bar.stowed #ui-bar-body {
	visibility: hidden;
	transition: visibility 200ms step-end;
}

/* ui bar information - story title, author name, subtitle */
#story-title {
	margin: 0;
	font-size: 162.5%;
}
#story-author {
	margin-top: 2em;
	font-weight: bold;
}
#story-subtitle {
}
/* ui bar list (menu links) */
#menu ul {
	margin: 0 0 0;
	padding: 0;
	list-style: none;
	border: none;
}
#menu ul:empty {
	display: none;
}
#menu li {
	margin: 0;
}
#menu li:not(:first-child) {
	border-top: none;
}
#menu li a {
	display: block;
	padding: 0.25em 0.75em;
	border: none;
	color: var(--link);
	text-transform: uppercase;
}
.dm #menu li a {
  color: var(--dm-link);
}
#menu li a:hover {
	background-color: var(--accent2);
}
.dm #menu li a:hover {
  background-color: var(--dm-accent2);
}

/* the buttons at the top of the ui bar that allow the player to collapse/expand it and to go back and forwards - to remove these, add display: none; as a line */
#ui-bar-toggle,
#ui-bar-history [id|="history"] {
	font-size: 1.2em;
	line-height: inherit;
	color: var(--text);
	background-color: transparent;
	border: none;
}
.dm #ui-bar-toggle, #ui-bar-history [id|="history"] {
  color: var(--dm-text);
}
#ui-bar-toggle {
	display: block;
	position: absolute;
	top: 0;
	right: 0;
	border-right: none;
	padding: 0.3em 0.45em 0.25em;
}
#ui-bar.stowed #ui-bar-toggle {
	padding: 0.3em 0.35em 0.25em 0.55em;
}
#ui-bar-toggle:hover {
	background-color: var(--accent2);
}
.dm #ui-bar-toggle:hover {
  background-color: var(--dm-accent2);
}
#ui-bar-history {
	margin: 0 auto;
}
#ui-bar-history [id|="history"] {
	padding: 0.2em 0.45em 0.35em;
  color: var(--link);
}
.dm #ui-bar-history [id|="history"] {
  color: var(--dm-link);
}
#ui-bar-history #history-jumpto {
	padding: 0.2em 0.665em 0.35em;
}
#ui-bar-history [id|="history"]:not(:first-child) {
	margin-left: 1.2em;
}
#ui-bar-history [id|="history"]:hover {
	background-color: var(--accent2);
}
.dm #ui-bar-history [id|="history"]:hover {
	background-color: var(--dm-accent2);
}

/* ui dialog box titlebars */
#ui-dialog-titlebar {
	background-color: var(--bg);
	min-height: 24px;
  border: 1px solid var(--accent);
}
.dm #ui-dialog-titlebar {
  background-color: var(--dm-bg);
  border: 1px solid var(--dm-accent);
}
#ui-dialog-title {
	margin: 0;
	padding: 0.2em 3.5em 0.2em 0.5em;
	font-size: 1.5em;
	text-align: center;
	text-transform: uppercase;
}
#ui-dialog-close {
	cursor: pointer;
	font-size: 120%;
	margin: 0;
	padding: 0;
	width: 3.6em;
	height: 92%;
	background-color: transparent;
	border: none;
	transition-duration: 200ms;
  color: black;
}
.dm #ui-dialog-close {
  color: white;
}
#ui-dialog-close:hover {
	background-color: var(--bg);
  color: var(--text);
}
.dm #ui-dialog-close:hover {
	background-color: var(--dm-bg);
  color: var(--dm-text);
}

/* the dialog boxes (settings, saves, other dialogs you add) */
#ui-dialog-body {
	background-color: var(--bg);
	border: 1px solid var(--accent);
	text-align: left;
	line-height: 1.5;
	padding: 1em;
}
.dm #ui-dialog-body {
  background-color: var(--dm-bg);
  border: 1px solid var(--accent);
}
#ui-dialog-body > *:first-child {
	margin-top: 0;
}
#ui-dialog-body hr {
	background-color: #000;
}

/* Default dialog button styling. */
#ui-dialog-body ul.buttons {
	margin: 0;
	padding: 0;
	list-style: none;
}
#ui-dialog-body ul.buttons li {
	display: inline-block;
	margin: 0;
	padding: 0.4em 0.4em 0 0;
}
#ui-dialog-body ul.buttons > li + li > button {
	margin-left: 1em;
}

/* selectors in settings */
#setting-control-theme, #setting-control-fontsize, #setting-control-fontfamily,  #setting-control-mastervolume {
    color: var(--text);
    background-color: var(--bg);
}
.dm #setting-control-theme, .dm #setting-control-fontsize, .dm #setting-control-fontfamily, .dm #setting-control-mastervolume {
    color: var(--dm-text);
    background-color: var(--dm-bg);
}

/* notifications bar */
#notify {
    position : fixed;
    display : block;
    width : 16em;
    right : -20em; top : 5%;
    padding : 0.5em;
    background-color : var(--bg);
    color : var(--text);
	border: 1px solid var(--accent);
    text-align: left;
    -webkit-transition : right 1s;
    -moz-transition : right 1s;
    -o-transition : right 1s;
    transition : right 1s;
}
.dm #notify {
  color: var(--dm-text);
  background-color: var(--dm-bg);
  border: 1px solid var(--dm-accent);
}
#notify.open { right : 0; }

/* buttons */

button {
  background-color: green;
}

/*scrollbar*/
 ::-webkit-scrollbar {
  width: 8px;
  }
 ::-webkit-scrollbar-track {
   -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0);
  border-radius: 10px,
 }
 ::-webkit-scrollbar-thumb {
   -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.6);
  border: 1px solid rgba(233, 231, 231, 0.4);
  border-radius:10px;
  width: 5px;
 }

/*Textbox*/

input, select, textarea {
	color: #000;
	background-color: transparent;
	border: 1px solid #444;
	padding: 0.4em;
}
  
.dm input, select, textarea {
	color: #fff;
	background-color: transparent;
	border: 1px solid #444;
	padding: 0.4em;
}

input:not(:disabled):focus, select:not(:disabled):focus, textarea:not(:disabled):focus,
input:not(:disabled):hover, select:not(:disabled):hover, textarea:not(:disabled):hover {
  background-color: #333;
  	color:#eee;
}

.dm input:not(:disabled):focus, select:not(:disabled):focus, textarea:not(:disabled):focus,
input:not(:disabled):hover, select:not(:disabled):hover, textarea:not(:disabled):hover {
  background-color: #333;
  	color:#eee;
}
